import unittest
import is_vegan

class TestIsVegan(unittest.TestCase):
    def test_empty_string(self):
        self.assertTrue(is_vegan.is_vegan_ingredient(''))

    def test_vegan_ingredient(self):
        self.assertTrue(is_vegan.is_vegan_ingredient('soy'))

    def test_non_vegan_ingredient(self):
        self.assertFalse(is_vegan.is_vegan_ingredient('milk'))

    def test_non_vegan_ingredient_multiple_words(self):
        self.assertFalse(is_vegan.is_vegan_ingredient('stilton cheese'))

    def test_partially_vegan_ingredient_multiple_words(self):
        self.assertTrue(is_vegan.is_vegan_ingredient('soy cheese'))

    def test_non_vegan_ingredient_list(self):
        self.assertFalse(is_vegan.is_vegan_ingredient_list(['aspic', 'albumin']))

    def test_vegan_ingredient_list(self):
        self.assertTrue(is_vegan.is_vegan_ingredient_list(['soy', 'cacao butter']))

    def test_contains_non_vegan_ingredient_list(self):
        self.assertEqual(is_vegan.contains_non_vegan_ingredients(['soy', 'cacao butter']), [])

    def test_contains_non_vegan_ingredient_list(self):
        self.assertCountEqual(is_vegan.contains_non_vegan_ingredients(['aspic', 'albumin', 'soy']), ['aspic', 'albumin'])

if __name__ == '__main__':
    unittest.main()
